<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="left_img" format="reference" /> <!--  左边图标-->
    <attr name="left_textStr" format="reference" /> <!--  左边文本-->
    <attr name="left_textId" format="reference" /><!-- 左边文本id-->
    <attr name="right_textStr" format="reference" /> <!--  右边文本-->
    <attr name="right_textId" format="reference" /><!-- 右边文本id-->
    <attr name="isShow_left_img" format="boolean" /> <!--  是否显示左边图标-->
    <attr name="right_img1" format="reference" /><!-- 右边图标1 -->
    <attr name="isShow_right_img1" format="boolean" /><!--是否显示右边图标1 -->
    <attr name="right_img2" format="reference" /><!-- 右边图标2 -->
    <attr name="isShow_right_img2" format="boolean" /><!--是否显示右边图标2 -->
    <attr name="right_img3" format="reference" /><!-- 右边图标3 -->
    <attr name="isShow_right_img3" format="boolean" /><!--是否显示右边图标3 -->
    <attr name="title_textStr" format="string" /><!-- title文本-->
    <attr name="title_textId" format="reference" /><!-- title文本-->
    <attr name="text_color" format="color" /> <!-- title文本 颜色-->
    <attr name="isShow_tab_layout" format="boolean" /> <!-- 是否显示tablayout-->
    <attr name="bg_color" format="color" /> <!-- 背景颜色-->
    <declare-styleable name="CustomNavigationBarView">
        <attr name="left_img" />
        <attr name="left_textStr" />
        <attr name="left_textId" />
        <attr name="right_textStr" />
        <attr name="right_textId" />
        <attr name="isShow_left_img" />
        <attr name="right_img1" />
        <attr name="isShow_right_img1" />
        <attr name="right_img2" />
        <attr name="isShow_right_img2" />
        <attr name="right_img3" />
        <attr name="isShow_right_img3" />
        <attr name="title_textStr" />
        <attr name="title_textId" />
        <attr name="text_color" />
        <attr name="isShow_tab_layout" />
        <attr name="bg_color" />
    </declare-styleable>
    <!-- 圆角矩形背景色 -->
    <attr name="rv_backgroundColor" format="color" />
    <!-- 圆角矩形渐变开始背景色 -->
    <attr name="rv_backgroundStartColor" format="color" />
    <!-- 圆角矩形渐变结束背景色 -->
    <attr name="rv_backgroundEndColor" format="color" />
    <!-- 圆角矩形背景色press -->
    <attr name="rv_backgroundPressColor" format="color" />
    <!-- 圆角矩形渐变开始背景色press -->
    <attr name="rv_backgroundPressStartColor" format="color" />
    <!-- 圆角矩形渐变结束背景色press -->
    <attr name="rv_backgroundPressEndColor" format="color" />
    <!-- 圆角弧度,单位dp-->
    <attr name="rv_cornerRadius" format="dimension" />
    <!-- 圆角弧度,单位dp-->
    <attr name="rv_strokeWidth" format="dimension" />
    <!-- 圆角边框颜色-->
    <attr name="rv_strokeColor" format="color" />
    <!-- 圆角边框颜色press -->
    <attr name="rv_strokePressColor" format="color" />
    <!-- 圆角边框破折线的宽度 -->
    <attr name="rv_strokeDashGap" format="dimension" />
    <!-- 圆角边框空隙的宽度 -->
    <attr name="rv_strokeDashWidth" format="dimension" />
    <!-- 文字颜色press-->
    <attr name="rv_textPressColor" format="color" />
    <!-- 圆角弧度是高度一半-->
    <attr name="rv_isRadiusHalfHeight" format="boolean" />
    <!-- 圆角矩形宽高相等,取较宽高中大值-->
    <attr name="rv_isWidthHeightEqual" format="boolean" />
    <!-- 圆角弧度,单位dp,TopLeft-->
    <attr name="rv_cornerRadius_TL" format="dimension" />
    <!-- 圆角弧度,单位dp,TopRight-->
    <attr name="rv_cornerRadius_TR" format="dimension" />
    <!-- 圆角弧度,单位dp,BottomLeft-->
    <attr name="rv_cornerRadius_BL" format="dimension" />
    <!-- 圆角弧度,单位dp,BottomRight-->
    <attr name="rv_cornerRadius_BR" format="dimension" />
    <!-- 是否有Ripple效果,api21+有效-->
    <attr name="rv_isRippleEnable" format="boolean" />

